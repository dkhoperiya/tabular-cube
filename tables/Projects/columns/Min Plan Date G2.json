{
  "type": "calculated",
  "name": "Min Plan Date G2",
  "dataType": "dateTime",
  "isDataTypeInferred": true,
  "expression": [
    "CALCULATE (",
    "    MIN ( 'Milestones'[Plan Date] ),",
    "    KEEPFILTERS ( 'Milestones'[Milestone] = \"[G2]\" )",
    ")"
  ],
  "formatString": "dd.MM.yyyy"
}