{
  "name": "FactIndFot_Lab",
  "description": "Списания Лабы без маржи",
  "expression": [
    "VAR DateForFot =",
    "    DATE ( 2022, 07, 01 )",
    "  ",
    "/*ФОТ по проектам с типом оплаты по ФОТ после 1 июл 2022*/",
    "VAR x1 =",
    "    CALCULATE (",
    "        [TB_Amount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"24\" ),",
    "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
    "    ) ",
    "    ",
    "/*ФОТ по проектам с типом оплаты по ТС после 1 июл 2022*/  ",
    "VAR x2 =",
    "    CALCULATE (",
    "        [FactFotRub],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"24\" } ),",
    "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
    "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
    "    ) /*ФОТ по проектам с типом оплаты по ФОТ и ТС до 1 июл 2022*/",
    "VAR x3 =",
    "    CALCULATE (",
    "        [FactFotRub],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"24\" } ),",
    "        KEEPFILTERS ( FactHours[IsRenormEnabled] IN { 0, 1 } ),",
    "        KEEPFILTERS ( Dates[Date] < DateForFot )",
    "    )",
    "RETURN",
    "    x1 + x2 + x3",
    "    + CALCULATE (",
    "        [WIP Project Value Open Period],",
    "        KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactIndFot_Lab\" )",
    "    )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики перенормализации\\Субподряд из Timesheets",
  "detailRowsDefinition": {
    "expression": [
      "VAR DateForFot =",
      "    DATE ( 2022, 07, 01 )",
      " ",
      "/*ФОТ по проектам с типом оплаты по ФОТ после 1 июл 2022*/",
      "VAR x1 =",
      "    CALCULATE (",
      "        [TB_Amount],",
      "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"24\" ),",
      "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
      "    )",
      "    ",
      "/*ФОТ по проектам с типом оплаты по ТС после 1 июл 2022*/",
      "VAR x2 =",
      "    CALCULATE (",
      "        [FactFotRub],",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"24\" } ),",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
      "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
      "    ) ",
      "    ",
      "/*ФОТ по проектам с типом оплаты по ФОТ и ТС до 1 июл 2022*/",
      "VAR x3 =",
      "    CALCULATE (",
      "        [FactFotRub],",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"24\" } ),",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] IN { 0, 1 } ),",
      "        KEEPFILTERS ( Dates[Date] < DateForFot )",
      "    )",
      "RETURN",
      "    ROW (",
      "        \"FotProjectsAfterRenormEpoch\", x1,",
      "        \"AllProjectsBeforeRenormEpoch\", x2,",
      "        \"TsProjectsAfterRenormEpoch\", x3,",
      "        \"FactIndFOTwoLeadersMTSD\",",
      "            x1 + x2 + x3",
      "    )"
    ]
  }
}