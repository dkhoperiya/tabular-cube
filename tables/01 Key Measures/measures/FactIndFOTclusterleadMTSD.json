{
  "name": "FactIndFOTclusterleadMTSD",
  "expression": [
    "VAR DateForFot =",
    "    DATE ( 2022, 7, 1 )",
    "  ",
    "/*Списания кластерлидов без маржи и накладных расходов по ФОТ*/",
    "VAR x1 =",
    "    CALCULATE (",
    "        [TB_Amount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[Calculation_TypeCode] IN { 2, 4 } ),",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" )",
    "    )",
    "    ",
    "/*Списания кластерлидов без маржи и накладных расходов по ТС*/",
    "VAR x2 =",
    "    CALCULATE (",
    "        [FactFotRub],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
    "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
    "        KEEPFILTERS ( Roles[RoleWorkAreaId] = 2 ),",
    "        KEEPFILTERS ( Dates[Date] >= DateForFot)",
    "    )",
    "    ",
    "RETURN",
    "    x1 + x2",
    "        + CALCULATE (",
    "            [WIP Project Value Open Period],",
    "            KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactIndFOTclusterleadMTSD\" )",
    "        )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики 2023"
}