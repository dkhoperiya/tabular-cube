{
  "name": "FactOverheadCostswoLeadersMTSD",
  "expression": [
    "VAR DateForUnitHead =",
    "    DATE ( 2023, 01, 01 )",
    "    ",
    "/*Накладные расходы от списаний по проектам ФОТ сотрудников Digital без биг боссов*/   ",
    "VAR x1 =",
    "    CALCULATE (",
    "        [TB_OverHeadCostAmount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[Calculation_TypeCode] IN { 1, 3 } ),",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" )",
    "    )",
    "    ",
    "/*Накладные расходы от списаний по проектам ТС сотрудников Digital без биг боссов но с юнитхедами до 01.01.2023*/   ",
    "VAR x2 =",
    "    CALCULATE (",
    "        [FactRubOverHeadCostAmount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
    "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
    "        KEEPFILTERS ( NOT Roles[RoleWorkAreaId] IN { 2 } ),",
    "        KEEPFILTERS ( Dates[Date] < DateForUnitHead)",
    "    )",
    "    ",
    "/*Накладные расходы от списаний по проектам ТС сотрудников Digital без биг боссов и без юнитхедов с 01.01.2023*/    ",
    "VAR x3 =",
    "    CALCULATE (",
    "        [FactRubOverHeadCostAmount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
    "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
    "        KEEPFILTERS ( NOT Roles[RoleWorkAreaId] IN { 2, 3 } ),",
    "        KEEPFILTERS ( Dates[Date] >= DateForUnitHead)",
    "    )",
    "    ",
    "RETURN",
    "    ( x1 + (x2 / 0.79) + (x3 / 0.79) )",
    "        + CALCULATE (",
    "            [WIP Project Value Open Period],",
    "            KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactOverheadCostswoLeadersMTSD\" )",
    "        )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики 2023"
}