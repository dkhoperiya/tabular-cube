{
  "name": "FactOverheadCostsAndMarginLeadersMTSD",
  "description": "Маржа и накладняк от списаний бигбоссов МТС Диджитал",
  "expression": [
    "CALCULATE (",
    "    [FactOverheadCostsClusterleadMTSD] + [FactMarginClusterleadMTSD] + [FactOverheadCostsUnitheadMTSD] + [FactMarginUnitheadMTSD],",
    "    'Options'[Wip History Mode] = \"As Is\"",
    ")",
    "    + CALCULATE (",
    "        [WIP Project Value Open Period],",
    "        KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactOverheadCostsAndMarginLeadersMTSD\" )",
    "    )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики перенормализации\\ФОТ ББ МТС Диджитал",
  "detailRowsDefinition": {
    "expression": [
      "VAR DateForFot =",
      "    DATE ( 2022, 07, 01 )",
      " ",
      "/*Маржа со списаний лидером по проектам с типом оплаты по ФОТ и ТС  до 1 июля 2022*/",
      "VAR x1 =",
      "    CALCULATE (",
      "        [FactRub] - [FactFotRub],",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] IN { 0, 1 } ),",
      "        KEEPFILTERS ( Roles[Is_Leader] = 1 ),",
      "        KEEPFILTERS ( Dates[Date] < DATE ( 2022, 7, 1 ) )",
      "    )",
      "    ",
      "/*Маржа со списаний лидером по проектам с типом оплаты ТС после 1 июля 2022*/    ",
      "VAR x2 =",
      "    CALCULATE (",
      "        [FactRub] - [FactFotRub],",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
      "        KEEPFILTERS ( Roles[Is_Leader] = 1 ),",
      "        KEEPFILTERS ( Dates[Date] >= DATE ( 2022, 7, 1 ) )",
      "    )",
      "    ",
      "/*Маржа со списаний лидером по проектам с типом оплаты ТС после 1 июля 2022*/    ",
      "VAR x3 =",
      "    CALCULATE (",
      "        [TB_Margin],",
      "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
      "        KEEPFILTERS ( 'TeamBudgeting_Hours_Calculation'[Calculation_TypeCode] IN { 2, 4 } ),",
      "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
      "    )",
      "    ",
      "RETURN",
      "    ROW (",
      "        \"Before_Renorm_Epoch\", x1,",
      "        \"TS_Projects_After_Renorm_Epoch\", x2,",
      "        \"FOT_Projects_After_Renorm_Epoch\", x3,",
      "        \"FactOverheadCostsAndMarginLeadersMTSD\",",
      "            x1 + x2 + x3",
      "    )"
    ]
  }
}