{
  "name": "Текущая укомплектованность",
  "description": "Отношение количества вакансий к количеству штатных позиций",
  "expression": "1 - DIVIDE ( [Текущее количество вакансий], [Текущее количество штатных единиц] )",
  "formatString": "#,##0.00 %",
  "displayFolder": "09. HR\\01. Текущая численность",
  "detailRowsDefinition": {
    "expression": [
      "VAR x =",
      "    CALCULATE ( [Текущее количество вакансий] )",
      "VAR y =",
      "    CALCULATE ( [Текущее количество штатных единиц] )",
      "RETURN",
      "    ROW (",
      "        \"Текущее количество вакансий\", x,",
      "        \"Текущее количество штатных единиц\", y,",
      "        \"Текущая укомплектованность\", 1 - DIVIDE ( x, y )",
      "    )"
    ]
  }
}