{
  "name": "FactIndFotLeadersMTSD",
  "description": "Списания бигбоссов  без маржи и накладняка",
  "expression": [
    "CALCULATE (",
    "    [FactIndFOTclusterleadMTSD] + [FactIndFOTunitheadMTSD],",
    "    'Options'[Wip History Mode] = \"As Is\"",
    ")",
    "    + CALCULATE (",
    "        [WIP Project Value Open Period],",
    "        KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactIndFotLeadersMTSD\" )",
    "    )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики перенормализации\\ФОТ ББ МТС Диджитал",
  "detailRowsDefinition": {
    "expression": [
      "VAR DateForFot =",
      "    DATE ( 2022, 07, 01 )",
      " ",
      "/*Проекты с оплатой по ФОТ после 1 июля*/",
      "VAR BB_FotRub_After_Renorm_Epoch_TB =",
      "    CALCULATE (",
      "        [TB_Amount],",
      "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[Calculation_TypeCode] IN { 2, 4 } ),",
      "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" ),",
      "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
      "    )",
      "  ",
      "/*Проекты с оплатой по ФОТ до 1 июля*/",
      "VAR BB_FotRub_Before_Renorm_Epoc_TS =",
      "    CALCULATE (",
      "        [FactFotRub],",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 1 ),",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" ),",
      "        KEEPFILTERS ( Roles[Is_Leader] = 1 ),",
      "        KEEPFILTERS ( Dates[Date] < DateForFot )",
      "    )",
      "    ",
      "/*Проекты с оплатой по ТС */",
      "VAR BB_FotRub_TS =",
      "    CALCULATE (",
      "        [FactFotRub],",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" ),",
      "        KEEPFILTERS ( Roles[Is_Leader] = 1 )",
      "    )",
      "  ",
      "RETURN",
      "    ROW (",
      "        \"BB_FotRub_After_Renorm_Epoch_TB \", BB_FotRub_After_Renorm_Epoch_TB,",
      "        \"BB_FotRub_Before_Renorm_Epoc_TS\", BB_FotRub_Before_Renorm_Epoc_TS,",
      "        \"BB_FotRub_TS\", BB_FotRub_TS,",
      "        \"FactIndFotLeadersMTSD\",",
      "            BB_FotRub_After_Renorm_Epoch_TB + BB_FotRub_Before_Renorm_Epoc_TS + BB_FotRub_TS",
      "    )"
    ]
  }
}