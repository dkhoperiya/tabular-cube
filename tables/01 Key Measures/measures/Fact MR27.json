{
  "name": "Fact MR27",
  "expression": [
    "VAR _INCOME_ITEMS_COUNT =",
    "    DISTINCTCOUNT ( 'PL Items'[PL Item Category Code] )",
    "RETURN",
    "    IF (",
    "        _INCOME_ITEMS_COUNT > 1,",
    "        BLANK (),",
    "        CALCULATE (",
    "            [Fact Project Expenses OEBS],",
    "            KEEPFILTERS ( 'FactProjectExpenses'[EXPENSE_CLASS] IN { \"OPEX\" } ),",
    "            KEEPFILTERS ( 'FactProjectExpenses'[IS_VGR_SUBCONTRACTOR] = FALSE () )",
    "        )",
    "            - CALCULATE (",
    "                [Fact Project Expenses OEBS],",
    "                KEEPFILTERS ( 'FactProjectExpenses'[EXPENSE_CLASS] IN { \"OPEX_INCOME\" } ),",
    "                KEEPFILTERS ( 'FactProjectExpenses'[IS_VGR_SUBCONTRACTOR] = FALSE () )",
    "            ) + [Sum of Debet with K]",
    "            + CALCULATE (",
    "                SUM ( 'Budget_MR27'[Budget] ),",
    "                KEEPFILTERS ( 'Budget_MR27'[FactOrPlan] = \"FACT\" )",
    "            )",
    "    )"
  ],
  "formatString": "#,##0",
  "displayFolder": "08. BIT Budget\\03. Budget MR27",
  "detailRowsDefinition": {
    "expression": [
      "VAR _OPEX =",
      "    CALCULATE (",
      "        [Fact Project Expenses OEBS],",
      "        KEEPFILTERS ( 'FactProjectExpenses'[EXPENSE_CLASS] = \"OPEX\" ),",
      "        KEEPFILTERS ( 'FactProjectExpenses'[IS_VGR_SUBCONTRACTOR] = FALSE () )",
      "    )",
      "VAR _FOT =",
      "    CALCULATE ( [Sum of Debet with K] )",
      "VAR _FIB =",
      "    CALCULATE (",
      "        SUM ( 'Budget_MR27'[Budget] ),",
      "        KEEPFILTERS ( 'Budget_MR27'[FactOrPlan] = \"FACT\" )",
      "    )",
      "VAR _INCOME =",
      "    - CALCULATE (",
      "        [Fact Project Expenses OEBS],",
      "        KEEPFILTERS ( 'FactProjectExpenses'[EXPENSE_CLASS] IN { \"OPEX_INCOME\" } ),",
      "        KEEPFILTERS ( 'FactProjectExpenses'[IS_VGR_SUBCONTRACTOR] = FALSE () )",
      "    )",
      "RETURN",
      "    ROW (",
      "        \"Opex\", _OPEX,",
      "        \"FOT\", _FOT,",
      "        \"FIB\", _FIB,",
      "        \"INCOME\", _INCOME,",
      "        \"Fact MR27\",",
      "            _OPEX + _FOT + _FIB + _INCOME",
      "    )"
    ]
  }
}