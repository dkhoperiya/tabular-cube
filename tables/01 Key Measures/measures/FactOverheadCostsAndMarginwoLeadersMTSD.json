{
  "name": "FactOverheadCostsAndMarginWoLeadersMTSD",
  "description": "Маржа и накладняк от списаний Диджитал без бигбоссов",
  "expression": [
    "CALCULATE (",
    "    [FactOverheadCostswoLeadersMTSD] + [FactMarginwoLeadersMTSD],",
    "    'Options'[Wip History Mode] = \"As Is\"",
    ")",
    "    + CALCULATE (",
    "        [WIP Project Value Open Period],",
    "        KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactOverheadCostsAndMarginWoLeadersMTSD\" )",
    "    )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики перенормализации\\Списания Диджитал с маржой и ББ",
  "detailRowsDefinition": {
    "expression": [
      "VAR DateForFot =",
      "    DATE ( 2022, 07, 01 )",
      "  ",
      "/*ФОТ по проектам с типом оплаты по ФОТ после 1 июл 2022*/",
      "VAR x1 = ",
      "    CALCULATE (",
      "        [TB_Margin],",
      "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[Calculation_TypeCode] IN { 1, 3 } ),",
      "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" ),",
      "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
      "    ) ",
      "    ",
      "/*ФОТ по проектам с типом оплаты по ТС после 1 июл 2022*/    ",
      "VAR x2 =",
      "    CALCULATE (",
      "        [FactRub] - [FactFotRub],",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
      "        KEEPFILTERS ( Roles[Is_Leader] <> 1 ),",
      "        KEEPFILTERS ( Dates[Date] >= DateForFot )",
      "    ) ",
      "    ",
      "/*ФОТ по проектам с типом оплаты по ФОТ и ТС до 1 июл 2022*/",
      "VAR x3 =",
      "    CALCULATE (",
      "        [FactRub] - [FactFotRub],",
      "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
      "        KEEPFILTERS ( FactHours[IsRenormEnabled] IN { 0, 1 } ),",
      "        KEEPFILTERS ( Roles[Is_Leader] <> 1 ),",
      "        KEEPFILTERS ( Dates[Date] < DateForFot )",
      "    )",
      "",
      "RETURN",
      "     ROW (",
      "        \"FotProjectsAfterRenormEpoch\", x1,",
      "        \"AllProjectsBeforeRenormEpoch\", x2,",
      "        \"TsProjectsAfterRenormEpoch\", x3,",
      "        \"FactOverheadCostsAndMarginWoLeadersMTSD\",",
      "            x1 + x2 + x3",
      "    )"
    ]
  }
}