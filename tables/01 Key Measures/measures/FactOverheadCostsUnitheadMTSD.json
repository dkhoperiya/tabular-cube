{
  "name": "FactOverheadCostsUnitheadMTSD",
  "expression": [
    "VAR DateForUnitHead =",
    "    DATE ( 2023, 01, 01 )",
    "",
    "/*Накладные расходы от списаний юнитхэдов по ФОТ*/",
    "VAR x1 =",
    "    CALCULATE (",
    "        [TB_OverHeadCostAmount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[Calculation_TypeCode] IN { 5, 6 } ),",
    "        KEEPFILTERS ( TeamBudgeting_Hours_Calculation[IsRenormEnabled] = 1 ),",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] = \"1\" )",
    "    )",
    "    ",
    "/*Накладные расходы от списаний юнитхэдов по ТС*/   ",
    "VAR x2 =",
    "    CALCULATE (",
    "        [FactRubOverHeadCostAmount],",
    "        'Options'[Wip History Mode] = \"As Is\",",
    "        KEEPFILTERS ( BusinessUnits[Entity Code] IN { \"1\" } ),",
    "        KEEPFILTERS ( FactHours[IsRenormEnabled] = 0 ),",
    "        KEEPFILTERS ( Roles[RoleWorkAreaId] = 3 ),",
    "        KEEPFILTERS ( Dates[Date] >= DateForUnitHead)",
    "    ) ",
    "RETURN",
    "    ( x1 + ( x2 / 0.79 ) )",
    "    + CALCULATE (",
    "        [WIP Project Value Open Period],",
    "        KEEPFILTERS ( 'WIP_ProjectMetricsHistory'[MeasureName] = \"FactOverheadCostsUnitheadMTSD\" )",
    "    )"
  ],
  "formatString": "#,##0.00",
  "displayFolder": "16. WIP\\Метрики 2023"
}